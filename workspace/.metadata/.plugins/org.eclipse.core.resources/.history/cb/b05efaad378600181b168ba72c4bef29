
public class displayOddApp {
	public static void main(String[] args) {

		int start = 1;
		int end = 100;

		for (int val = start; val < end; val++)
		{
		 
		    if (val % 2 == 0) 
		    {
		        System.out.println();
		    }
		    else
		    {
		        System.out.println(val);
		    }
		}
	
	}
}
