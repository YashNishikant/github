
public class App {
	public static void main(String[] args) {
		String[] words = new String[3];
		
		words[0] = "Hello";
		words[1] = "To";
		words[2] = "You";
	
		System.out.println(words[2]);
	
		String[] foods = {"Apple, Bananna, Kiwi, Orange"};
	
		for(String food: foods) {
		System.out.println(food);
		
		}
	}
}